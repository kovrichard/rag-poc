{
  "name": "rag-poc",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun --hot run src/index.ts",
    "type-check": "tsc --noEmit",
    "lint-staged": "lint-staged",
    "lint-check": "biome lint --write ./src",
    "format": "biome format --write ./src",
    "prepare": "husky",
    "postinstall": "prisma generate",
    "migrate": "prisma migrate dev"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bun": "latest",
    "@types/cors": "2.8.17",
    "@types/express": "5.0.0",
    "@types/multer": "1.4.12",
    "husky": "9.1.7",
    "lint-staged": "15.4.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "2.2.2",
    "@prisma/client": "6.3.0",
    "cors": "2.8.5",
    "express": "4.21.2",
    "joi": "17.13.3",
    "multer": "1.4.5-lts.1",
    "openai": "4.82.0",
    "prisma": "6.3.0"
  }
}
